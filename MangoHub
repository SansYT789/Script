--// Load WindUI
local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/windui.lua"))()

--// Tạo main window vừa điện thoại
local MainWindow = WindUI:CreateWindow({
    Title = "Main",             -- Tên cửa sổ
    Size = UDim2.new(0, 350, 0, 200),  
    Theme = "Mango"
})

--// Thêm text hiển thị
MainWindow:AddLabel({
    Text = "Welcome to MangoHub! 🍊",
})

MainWindow:AddLabel({
    Text = "Script Made By Vinreach",
})
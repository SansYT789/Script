    Size = UDim2.new(0, 350, 0, 250),
    Theme = "Dark",
    Icon = "AirPlay"   -- ðŸ”¹ icon Ä‘áº§u window
})

--// ThÃªm tab Main vá»›i icon House
local MainTab = Window:Tab({
    Title = "Main",
    Icon = "House",    -- ðŸ”¹ icon tab Main
    Locked = false
})
--// Load WindUI
local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/RoyRedRedVN/Library-Ui-For-Script-Hacking/refs/heads/main/windui.lua"))()

--// T·∫°o main window chu·∫©n
local Window = WindUI:CreateWindow({
    Title = "MangoHub | Demo Version",
    Icon = "door-open",
    Author = "MADE BY TEAM VINREACH",
    Folder = "MangoHub",
    Size = UDim2.new(0, 350, 0, 250),
    Theme = "Mango"
})

--// Th√™m label
local MainLabel = Window:AddLabel({ Text = "Welcome to MangoHub! üçä" })
local MadeByLabel = Window:AddLabel({ Text = "Script Made By Vinreach" })

--// T·∫°o tab demo
local DemoTab = Window:AddTab({ Title = "Demo" })

--// Th√™m n√∫t demo
local ClickButton = DemoTab:AddButton({
    Text = "Click Me!",
    Callback = function()
        print("Button clicked!")
    end
})

--// Th√™m toggle demo
local EnableToggle = DemoTab:AddToggle({
    Title = "Enable Feature",
    Default = false,
    Callback = function(value)
        print("Toggle state:", value)
    end
})

--// Th√™m slider demo
local VolumeSlider = DemoTab:AddSlider({
    Title = "Volume",
    Min = 0,
    Max = 100,
    Default = 50,
    Callback = function(value)
        print("Volume set to", value)
    end
})

--// Th√™m input demo
local NameInput = DemoTab:AddInput({
    Title = "Enter Name",
    Placeholder = "Type here...",
    Callback = function(text)
        print("Input:", text)
    end
})